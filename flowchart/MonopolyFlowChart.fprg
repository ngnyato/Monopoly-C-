    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numRegistered, numInGame, turnIndex, freeParkValue" type="Integer" array="False" size=""/>
            <comment text="Declarar Grupo A&#13;&#10;"/>
            <assign variable="numRegistered" expression="0"/>
            <assign variable="numInGame" expression="0"/>
            <assign variable="turnIndex" expression="0"/>
            <assign variable="freeParkValue" expression="0"/>
            <declare name="gameInProgress" type="Boolean" array="False" size=""/>
            <comment text="Declarar Grupo B&#13;&#10;"/>
            <assign variable="gameInProgress" expression="false"/>
            <comment text="Grupo C"/>
            <declare name="RegName" type="String" array="True" size="50"/>
            <comment text="Grupo D"/>
