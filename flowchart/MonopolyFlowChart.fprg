            <comment text="Grupo H Tabuleiro 7x7 (strings)&#13;&#10;"/>
            <declare name="HouseName, HouseType, HouseColor" type="String" array="True" size="49"/>
            <comment text="Grupo I Tabuleiro 7x7 (ints)&#13;&#10;"/>
            <declare name="HousePrice, HouseRent, HouseOwnerIdx, HouseNum" type="Integer" array="True" size="49"/>
            <call expression="InitializeBoard(HouseName,HouseType,HouseColor,HousePrice,HouseRent,HouseOwnerIdx,HouseNum)"/>
