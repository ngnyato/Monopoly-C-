    <function name="InitializeBoard" type="None" variable="">
        <parameters>
            <parameter name="HouseName" type="String" array="True"/>
            <parameter name="HouseType" type="String" array="True"/>
            <parameter name="HouseColor" type="String" array="True"/>
            <parameter name="HousePrice" type="Integer" array="True"/>
            <parameter name="HouseRent" type="Integer" array="True"/>
            <parameter name="HouseOwnerIdx" type="Integer" array="True"/>
            <parameter name="HouseNum" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="48" direction="inc" step="1">
                <assign variable="HouseOwnerIdx[i]" expression="-1"/>
                <assign variable="HouseNum[i]" expression="0"/>
                <assign variable="HouseName[i]" expression="&quot;&quot;"/>
                <assign variable="HouseType[i]" expression="&quot;&quot;"/>
                <assign variable="HousePrice[i]" expression="0"/>
                <assign variable="HouseRent[i]" expression="0"/>
                <assign variable="HouseColor[i]" expression="&quot;&quot;"/>
            </for>
            <declare name="idx" type="Integer" array="False" size=""/>
            <assign variable="idx" expression="3*7+3"/>
            <assign variable="HouseName[idx]" expression="&quot;Start&quot;"/>
            <assign variable="HouseType[idx]" expression="&quot;Special&quot;"/>
            <assign variable="idx" expression="0*7+0"/>
            <assign variable="HouseName[idx]" expression="&quot;Prison&quot;"/>
            <assign variable="HouseType[idx]" expression="&quot;Special&quot;"/>
            <assign variable="idx" expression="6*7+6"/>
            <assign variable="HouseName[idx]" expression="&quot;Police&quot;"/>
            <assign variable="HouseType[idx]" expression="&quot;Special&quot;"/>
            <assign variable="idx" expression="6*7+0"/>
            <assign variable="HouseName[idx]" expression="&quot;Freepark&quot;"/>
            <assign variable="HouseType[idx]" expression="&quot;Special&quot;"/>
        </body>
    </function>
    <function name="RegistrarJogador" type="Boolean" variable="">
